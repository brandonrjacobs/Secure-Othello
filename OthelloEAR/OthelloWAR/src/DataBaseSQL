CREATE DATABASE OTHELLOGAME;
USE OTHELLOGAME;




-- FOR TESTING PURPOSE 

CREATE TABLE student (
  id int(11) NOT NULL AUTO_INCREMENT,
  firstname varchar(20) NOT NULL,
  age  int(2) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;


